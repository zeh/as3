					}

					if (!isNaN(similarity) && similarity <= maximumError && (similarity < closestPathSimilarity || closestPath == -1)) {
						// This one is closer
						closestPathSimilarity = similarity;
						closestPath = i;
					}
				}
			}

			//debug("Spent " + (getTimer() - ti) + "ms to recognize gesture -- closestPathSimilarity = " + closestPathSimilarity);

			return closestPath > -1 ? knownGestureIds[closestPath] : null;
		}


		// ================================================================================================================
		// EVENT INTERFACE ------------------------------------------------------------------------------------------------

		private function onMouseDown(__e:Event):void {
			startRecording();
		}

		private function onMouseMove(__e:Event):void {
			recordPoint();
		}

		private function onMouseUp(__e:Event):void {
			endRecording();
		}


		// ================================================================================================================
		// PUBLIC INTERFACE -----------------------------------------------------------------------------------------------

		public function getPreviousGestureId():String {
			return previousGestureId;
		}

	}
}
