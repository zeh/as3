					var pathNormalized:Path = path.clone();
					pathNormalized.normalize();

					previousStrokesNormalized.push(pathNormalized);

					if (previousStrokesRaw.length > maxStrokes) {
						previousStrokesRaw.splice(0, previousStrokesRaw.length - maxStrokes);
						previousStrokesNormalized.splice(0, previousStrokesNormalized.length - maxStrokes);
					}

					previousGestureId = findClosestGestureId();

					dispatchEvent(new Event(EVENT_GESTURE_COMPLETED));
